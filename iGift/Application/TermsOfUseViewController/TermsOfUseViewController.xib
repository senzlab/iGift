                                            <rect key="frame" x="0.0" y="1477" width="375" height="14.5"/>
                                            <string key="text">10.	Governing Law</string>
                                            <fontDescription key="fontDescription" type="boldSystem" pointSize="12"/>
                                            <nil key="textColor"/>
